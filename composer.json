{
  "name": "snipe/snipe-it",
  "description": "Asset Management System",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "fideloper/proxy": "^4.4",
  "laravel/framework": "^9.0",
  "laravel/tinker": "^2.7",
    "guzzlehttp/guzzle": "^7.0.1",
    "doctrine/dbal": "^3.0",
    "maatwebsite/excel": "^3.1",
  "spatie/laravel-activitylog": "^4.6",
  "spatie/laravel-backup": "^7.7",
  "spatie/laravel-permission": "^5.5",
  "spatie/laravel-translatable": "^6.0",
  "spatie/laravel-medialibrary": "^9.0",
  "spatie/laravel-sluggable": "^2.6",
  "spatie/laravel-query-builder": "^4.0",
  "spatie/laravel-html": "^2.0",
  "spatie/laravel-ignition": "^2.0",
  "spatie/laravel-sitemap": "^5.8",
  "spatie/laravel-newsletter": "^4.0",
  "spatie/laravel-cookie-consent": "^2.0",
  "spatie/laravel-csp": "^2.0",
  "spatie/laravel-ray": "^1.0",
  "spatie/laravel-responsecache": "^6.2",
  "spatie/laravel-server-monitor": "^1.0",
  "spatie/laravel-webhook-client": "^2.6",
  "spatie/laravel-webhook-server": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.4",
    "fakerphp/faker": "^1.9.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
     "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
    "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
    "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
